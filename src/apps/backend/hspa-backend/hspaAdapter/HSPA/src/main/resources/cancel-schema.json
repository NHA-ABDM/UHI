            "id": {
              "type": "string"
            },
            "state": {
              "type": "string"
            },
            "fulfillment": {
              "type": "object",
              "properties": {
                "tags": {
                  "type": "object",
                  "properties": {
                    "@abdm/gov.in/cancelledby": {
                      "type": "string"
                    },
                    "@abdm/gov.in/cancelReason": {
                      "type": "string"
                    },
                    "@abdm/gov.in/groupConsultation": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": false,
                  "required": [
                    "@abdm/gov.in/cancelledby",
                    "@abdm/gov.in/cancelReason",
                    "@abdm/gov.in/groupConsultation"
                  ]
                }
              },
              "additionalProperties": false,
              "required": [
                "tags"
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "id",
            "state",
            "fulfillment"
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "order"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "context",
    "message"
  ]
